type Query {
    userById(userId: ID!): User!
}

type User {
    id: ID!
    firstName: String!
    lastName: String!
    username: String!
    password: String!
    address: Address!
}

type Address {
    id: ID!
    address1: String!
    address2: String
    city: String!
    state: String!
    postal: String!
}